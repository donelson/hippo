<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item>
		<shape
			android:shape="rectangle"
			android:id="@+id/rectangle" >
			
			<padding
				android:left="1dip"
				android:right="1dip"
				android:bottom="1dip"
				android:top="1dip">
			</padding>
			<corners
			    android:radius="5dip"/>
			<stroke
				android:bottom="2dip"
				android:left="1dip"
				android:right="2dip"
				android:top="1dip" 
				android:color="#FFFFFF"/>
			<solid 
				android:color="#000000" />
		</shape>
	</item>
</layer-list>